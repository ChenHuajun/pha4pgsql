#!/bin/bash

###########################################################
#unmanage_all_resources.sh
#InPut:无
#OutPut:无
#
#return
#0  正常
#1  一般错误
#101 无法获取crm信息，比如pacemaker,corosync服务未启动(保留)
#102 分区未达到法定票数或和任意一个另外的HA节点状态不一致(保留)
###########################################################

cd $(dirname "$0")
. ../lib/common.sh


# unmanage all resources
# manage all resource
pcs resource unmanage $RESOURCE_LIST
